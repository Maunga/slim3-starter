{% extends 'base.twig' %}
{% block content %}

    <h1>List of articles</h1>
    <ul>
        {% for article in  articles %}
            <li><a href="{{ path_for('article.show', {id: article.id} ) }}">{{ article.title }}</a></li>
        {% endfor %}
    </ul>

    {{ articles.links | raw }}

{% endblock %}



